        app:layout_constraintTop_toTopOf="@+id/textViewTemperaturaMax" />

    <TextView
        android:id="@+id/textViewTemperatura2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        app:layout_constraintEnd_toStartOf="@+id/textViewCity2"
